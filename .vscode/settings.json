// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/*.js": { "when": "$(basename).ts" },
        "node_modules": true,
        ".sass-cache": true,
        "**/*.js.map": true,
        "images": true,
        "dist": true
    },
    "typescript.tsdk": "node_modules/typescript/lib"
}
