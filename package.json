{
  "name": "devday",
  "version": "1.0.0",
  "description": "The site for devday",
  "scripts": {
    "build": "cross-env NODE_ENV=production API_ENDPOINT=http://devday.in/ webpack",
    "tests:unit": "cross-env TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' mocha -r ts-node/register -r ignore-styles -r jsdom-global/register src/**/*.spec.ts",
    "tests:integration": "cypress run",
    "test": "npm run tests:unit && npm run tests:integration",
    "dev": "cross-env NODE_ENV=development API_ENDPOINT=http://devday.in/ webpack-dev-server --content-base ./dist --hot --inline --colors --port 3030 --open --history-api-fallback",
    "start": "npm run dev"
  },
  "keywords": [
    "devday"
  ],
  "author": "devday_ team at Sahaj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artfuldev/devday/issues"
  },
  "homepage": "https://github.com/artfuldev/devday",
  "dependencies": {
    "@cycle/dom": "^18.3.0",
    "@cycle/history": "^6.4.0",
    "@cycle/http": "^14.3.0",
    "@cycle/isolate": "^3.1.0",
    "@cycle/run": "^3.2.0",
    "marked": "^0.3.6",
    "material-design-lite": "^1.3.0",
    "moment": "^2.18.1",
    "switch-path": "^1.2.0",
    "xstream": "^10.9.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/history": "^4.6.0",
    "@types/marked": "^0.3.0",
    "@types/mocha": "^2.2.43",
    "autoprefixer-loader": "^3.2.0",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.23.1",
    "cssnano": "^3.10.0",
    "cypress": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.2.0",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.7",
    "lite-server": "^2.2.2",
    "mocha": "^3.5.3",
    "node-sass": "^3.8.0",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^4.1.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.13.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "yargs": "^9.0.1"
  }
}
